{
  "kind": "collectionType",
  "collectionName": "items",
  "info": {
    "singularName": "item",
    "pluralName": "items",
    "displayName": "item"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "mrp": {
      "type": "integer"
    },
    "sellingprice": {
      "type": "integer"
    },
    "quantity": {
      "type": "string"
    },
    "slug": {
      "type": "uid"
    },
    "images": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "items"
    },
    "user_carts": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::user-cart.user-cart",
      "mappedBy": "items"
    }
  }
}
